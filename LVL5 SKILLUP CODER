------------------------------------------------------------------TYPE:EASY------------------------------------------------------------------------------------------

--->HARSHED NUMBER

#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,sum=0,t;
    cin>>n;
    t=n;
    while(n!=0){
        sum=sum+n%10;
        n=n/10;
    }
    if(t%sum==0){
        cout<<"True";
    }
    else{
        cout<<"False";
    }
}


--->HAPPY NUMBER            NOT DONE!!!



--->GCD OR HCF

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n1,n2,high;
    cin>>n1>>n2;
    for (int i=1;i<=n1;i++){
        if(n1%i==0 && n2%i==0){
            high=i;
        }
    }
    cout<<high;
}


--->FIBONACCI OR NOT

#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,i1=0,i2=1,fib=0,c=0;
    cin>>n;
    for(int i=1;i<n;i++){
        fib=i1+i2;
        if(fib==n){
            cout<<"True";
            c=1;
            break;
        }
        i1=i2;
        i2=fib;
    }
    if(c==0){
        cout<<"False";
    }
}


--->FIBBONACCI


#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,i1=0,i2=1,fib=0;
    cin>>n;
    cout<<i1<<" "<<i2<<" ";
    for(int i=0;i<=(n-3);i++)
    {
        fib=i1+i2;
        cout<<i1+i2<<" ";
        i1=i2;
        i2=fib;
        
    }
}

