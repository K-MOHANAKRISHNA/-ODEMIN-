-------------------------------------------------LEVEL-1------------------------------------------------------------
-------------------------------------------------EASY---------------------------------------------------------------
--->INTEGER DIVISION

##PYTHON SOLUTION

x=int(input())
print(x//10)


--->AREA OF CIRCLE


//C++ SOLUTION

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int r;
    cin>>r;
    cout<<fixed<<setprecision(2)<<3.14*r*r;
}


##PYTHON SOLUTION

r=int(input())
print(round(3.14*r*r,2))

//C SOLUTION

#include<stdio.h>
int main()
{
    int r;
    scanf("%d",&r);
    printf("%.2f",3.14*r*r);
}


--->COMPOUND INTREST

//C++ SOLUTION

#include<bits/stdc++.h>
using namespace std;
int main()
{
    float p,r,t;
    cin>>p>>r>>t;
    cout<<fixed<<setprecision(2)<<p*pow((1+r/100.0),t);
}


--->SIMPLE INTREST

//C++ SOLUTION

#include<iostream>
using namespace std;
int main()
{
    int p,t,r;
    cin>>p>>t>>r;
    cout<<(p*t*r)/100;
}

##PYTHON SOLUTION

s=input()
x=s.split()
p=int(x[0])
r=int(x[1])
t=int(x[2])
print((p*r*t)//100)

-------------------------------------------------------MEDIUM----------------------------------------------------

--->ADD DIGITS

//C++ SOLUTION

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,sum=0;
    cin>>n;
    while(n>0 || sum>9)
    {
        if(n==0)
        {
            n=sum;
            sum=0;
            
        }
        sum=sum+n%10;
        n=n/10;
        
    }
    cout<<sum;
}


--->PERFECT NUMBER

//c++ SOLUTION

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int sum=0;
    for (int i =1;i<n;i++)
    {
        if(n%i==0)
        {
            sum=sum+i;
        }
    }
    if(sum==n){
        cout<<"True";
    }
    else
    {
        cout<<"False";
    }
}
