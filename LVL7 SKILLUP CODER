--->PRIME NUMBER

#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,c=0;
    cin>>n;
    for ( int i=1;i<=n;i++){
        if(n%i==0){
            c=c+1;
        }
    }
    if(c==2){
        cout<<"prime";
    }
    else{
        cout<<"not a prime";
    }
}


--->MEGA PRIMES     NOTE------SOME TESTCASES FAILED

#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,c1=0,t,dig=0,c2=0,cnt=0;
    cin>>n;
    t=n;
    while(n!=0){
        c1=c1+1;
        n=n/10;
    }
    n=t;
    for(int i=1;i<=c1;i++){
        while(n!=0){
            dig=n%10;
            for(int j=1;j<=dig;j++){
                if(dig%j==0){
                    cnt=cnt+1;
                }
            }
            if(cnt==2){
                c2=c2+1;
                cnt=0;
            }
            n=n/10;
        }
    }
    if(c1==c2){
        cout<<"Mega Prime";
    }
    else{
        cout<<"Not Mega Prime";
    }
}
